{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vennivijadf"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LS_adls_sql_db_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adls_sql_db'"
		},
		"LS_sink_diff_dates_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_sink_diff_dates'"
		},
		"LS_sql_db_1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sql_db_1'"
		},
		"LS_sqldb_2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sqldb_2'"
		},
		"inputblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'inputblob'"
		},
		"outputdatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'outputdatabase'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=saivennelasqlserver.database.windows.net;Initial Catalog=vennelasqldb;User ID=@{linkedService().sqlusername};Password=@{linkedService().sqluserpwd}"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vennela12.dfs.core.windows.net/"
		},
		"LS_adls_sql_db_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vennela12.dfs.core.windows.net/"
		},
		"LS_sink_diff_dates_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vennela12.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vennivijadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LS_adls_sql_db_accountKey": {
						"value": "[parameters('LS_adls_sql_db_accountKey')]"
					},
					"LS_sink_diff_dates_accountKey": {
						"value": "[parameters('LS_sink_diff_dates_accountKey')]"
					},
					"LS_sql_db_1_connectionString": {
						"value": "[parameters('LS_sql_db_1_connectionString')]"
					},
					"LS_sqldb_2_connectionString": {
						"value": "[parameters('LS_sqldb_2_connectionString')]"
					},
					"inputblob_connectionString": {
						"value": "[parameters('inputblob_connectionString')]"
					},
					"outputdatabase_connectionString": {
						"value": "[parameters('outputdatabase_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"LS_adls_sql_db_properties_typeProperties_url": {
						"value": "[parameters('LS_adls_sql_db_properties_typeProperties_url')]"
					},
					"LS_sink_diff_dates_properties_typeProperties_url": {
						"value": "[parameters('LS_sink_diff_dates_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "vennivijadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'vennivijadf_ArmTemplate_0')]"
			]
		}
	]
}