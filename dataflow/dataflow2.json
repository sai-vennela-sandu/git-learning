{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_var_adls",
						"type": "DatasetReference"
					},
					"name": "SOURCE1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_var_db",
						"type": "DatasetReference"
					},
					"name": "SINK1",
					"rejectedDataLinkedService": {
						"referenceName": "LS_adls_db",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          RNO as string,",
				"          Serial_Number as string,",
				"          Company_Name as string,",
				"          Employee_Markme as string,",
				"          Descriptions as string,",
				"          Leave as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 613,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['sample/orders_2022-12-24.csv','Archive_task']) ~> SOURCE1",
				"SOURCE1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          orderid as integer,",
				"          ordername as string,",
				"          orderprice as integer,",
				"          ordertype as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     preCommands: [],",
				"     postCommands: []) ~> SINK1"
			]
		}
	}
}