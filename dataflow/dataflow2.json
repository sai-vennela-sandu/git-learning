{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_M_SOURCE_ADLS_DB",
						"type": "DatasetReference"
					},
					"name": "SOURCE1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_M_SINK_DB",
						"type": "DatasetReference"
					},
					"name": "SINK1",
					"rejectedDataLinkedService": {
						"referenceName": "LS_adls_db",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          orderid as string,",
				"          ordername as string,",
				"          orderprice as string,",
				"          ordertype as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 613,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['sample','Archive_task'],",
				"     rowUrlColumn: ($ORDER_DATE)) ~> SOURCE1",
				"SOURCE1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Order_id as string,",
				"          Order_name as string,",
				"          Order_price as string,",
				"          Order_type as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Order_id = orderid,",
				"          Order_name = ordername,",
				"          Order_price = orderprice,",
				"          Order_type = ordertype",
				"     ),",
				"     preCommands: [],",
				"     postCommands: []) ~> SINK1"
			]
		}
	}
}