{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dataset_sp_sqldb",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "ls_sp_blob",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     parameter1 as string (city(replace(city, vij, mysore))",
				"}",
				"source(output(",
				"          order_id as integer,",
				"          order_name as string,",
				"          order_price as string,",
				"          order_type as string,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: true,",
				"     waterMarkColumn: 'city_orderss',",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select city',",
				"     format: 'query') ~> source1",
				"source1 derive(city_order = ERROR_FUNCTION('')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     dateFormat:'26-12-2022',",
				"     timestampFormat:'6.00',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     outputAssertFailedRows: true) ~> sink1"
			]
		}
	}
}