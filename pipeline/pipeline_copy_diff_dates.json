{
	"name": "pipeline_copy_diff_dates",
	"properties": {
		"activities": [
			{
				"name": "Copydata_diff_dates",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
